{
  "compilerOptions": {
    "target": "ESNext",
    "lib": ["ES2021", "dom"],
    "forceConsistentCasingInFileNames": true,
    "allowJs": true,
    "strict": true,
    "isolatedModules": true,
    "skipLibCheck": true,
    "moduleResolution": "Bundler",
    "module": "ESNext"
  },
  "include": ["./**/*"],
  "exclude": ["./_generated"]
}
